{
    "version":2,
    "name":"petgram-nadia",
    "builds":[
        {"use":"@now/static-build",
        "src":"package.json"}
    ],
    "routes":[
        {
            "src":"(.*).js", "dest":"/$1.js"
        },
        {
            "src":"(.*).json", "dest":"/$1.json"
        },
        {
            "src":"/.*", "dest":"/inedex.html"
        }
    ]
}